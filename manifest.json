{
"manifest_version": 2,
"name": "SkyHttpHeader",
"description": "Check http header. Copy clipborad.",
"version": "0.0.2",
"homepage_url": "https://github.com/nagoring/SkyHttpHeader",
"browser_action": {
	"default_icon": "img/icon.png",
	"default_popup": "popup.html"
},
"permissions": [ "webRequest", "storage", "webRequestBlocking", "tabs", "http://*/", "https://*/", "clipboardRead" ],
"background": {
	"scripts": ["js/background.js"]
}
}